<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginBottom="10dp"
    android:padding="10dp"
    android:background="@color/colorFondoInfo"
    android:id="@+id/rvListaTestsLink"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rvListaTestsFecha"
        android:text="Fecha del test"
        app:layout_constraintBottom_toTopOf="@id/rvListaTestsResultado" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rvListaTestsResultado"
        android:text="Resultado"
        app:layout_constraintTop_toBottomOf="@id/rvListaTestsFecha" />
</androidx.constraintlayout.widget.ConstraintLayout>